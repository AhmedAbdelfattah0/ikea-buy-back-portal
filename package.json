{
  "name": "buyback-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sa-qa": "ng build --base-href=/sa/en/buyback/ --c=sa-qa",
    "sa-qa-ar": "ng build --base-href=/sa/ar/buyback/ --c=sa-qa-ar",
    "sa-prod": "ng build --base-href=/sa/en/buyback/ --c=sa-prod",
    "sa-prod-ar": "ng build --base-href=/sa/ar/buyback/ --c=sa-prod-ar",
    "bh-qa": "ng build --base-href=/bh/en/buyback/ --c=bh-qa",
    "bh-qa-ar": "ng build --base-href=/bh/ar/buyback/ --c=bh-qa-ar",
    "bh-prod": "ng build --base-href=/bh/en/buyback/ --c=bh-prod",
    "bh-prod-ar": "ng build --base-href=/bh/ar/buyback/ --c=bh-prod-ar"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
