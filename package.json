    "@ingka/icon-webc": "^0.13.4",
    "@ingka/input-field-webc": "^0.9.4",
    "@ingka/modal-webc": "^0.12.5",
    "@ingka/pill-webc": "^0.11.2",
    "@ingka/price-webc": "^0.9.4",
    "@ingka/product-identifier-webc": "^0.7.2",
    "@ingka/quantity-stepper-webc": "^0.11.4",
    "@ingka/search-webc": "^0.0.4",
    "@ingka/toast-webc": "^0.12.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
